<p id="notice"><%= notice %></p>

<h1 class="title">Listing Products</h1>



<div class="row">
  <tbody class="table">
    <% @products.each do |product| %>
    <div class="col-sm-3">
    <div class="panel panel-default">
      <div class="panel-body">
          <div class="product">
            <%= link_to image_tag(product.thumb), product %></br>
            <%= product.name %></br>
            <%= product.mfg %></br>
            <%= link_to 'Visit Site', product.url %></br>
            <%= link_to 'Show', product %>
          </div>
        </div>
      </div>
    </div>
    <% end %>
</div>
</br>

<%= link_to 'New Product', new_product_path %>
